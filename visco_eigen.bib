@article{BRUINSMA1990287,
title = "A fast algorithm to compute the {$\mathcal H_\infty$}-norm of a transfer function matrix",
journal = "Syst. Control Lett.",
volume = "14",
number = "4",
pages = "287 - 293",
year = "1990",
issn = "0167-6911",
doi = "https://doi.org/10.1016/0167-6911(90)90049-Z",
url = "http://www.sciencedirect.com/science/article/pii/016769119090049Z",
author = "N.A. Bruinsma and M. Steinbuch",
keywords = ", Hamimtonian matrix, singular values",
abstract = "A fast algorithm is presented to compute the H∞-norm of a transfer function matrix, based on the relation between the singular values of the transfer function matrix and the eigenvalues of a related Hamiltonian matrix. The norm is computed with guaranteed accuracy. A very simple method to compute a rather close lower bound on the H∞-norm is given."
}

